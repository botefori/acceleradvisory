{% extends 'base.html.twig' %}

{% block body %}
    <h1>Acc_users_group</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ acc_users_group.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ acc_users_group.name }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('usergroups_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('usergroups_edit', { 'id': acc_users_group.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
